{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Package",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceRoot}",
      "console": "integratedTerminal"
    },
    {
      "name": "Debug Lexer",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/lexer/lexer_test.go"
    },
    {
      "name": "Debug Parser",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/parser/parser_test.go",
      "args": ["./..."]
    },
    {
      "name": "Debug AST",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/ast/ast_test.go"
    },
    {
      "name": "Debug Evaluator",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/evaluator/evaluator_test.go",
      "console": "integratedTerminal"
    }
  ]
}
